{
    "name": "diglabby/doika",
    "type": "project",
    "description": "Doika",
    "keywords": [
        "vue",
        "laravel",
        "bePaid"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.1.3",
        "ext-PDO": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "anlutro/l4-settings": "^1.0",
        "carbondate/carbon": "^1.33",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.22",
        "moneyphp/money": "^3.2",
        "spatie/laravel-query-builder": "^2.0",
        "tightenco/ziggy": "^1.0.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.2",
        "diglabby/laravel-find-missing-translations": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^5.0",
        "laravel/dusk": "^5.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5 | 8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
          "php": "7.1.12"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Diglabby\\Doika\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "bootstrap/helper_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/diglabby/laravel-find-missing-translations"
      }
    ],
    "scripts": {
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "php:fix": "php-cs-fixer fix --no-interaction --ansi --verbose",
        "test": "./vendor/bin/phpunit --colors=always --exclude-group=real-api",
        "test:ci": "@test"
    }
}
